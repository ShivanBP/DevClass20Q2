<apex:page controller="EventListPageController">
    <apex:pageBlock >
        <apex:pageBlockSection title="Event List" columns="1">
            <apex:pageBlockTable value="{!Events}" var="e">
                <apex:column headerValue="Account " >
                    <apex:outputlink value="{!THIS_ORG_URL}/apex/RegistrationPage?Id={!e.Id}" id="link">{!e.Name}</apex:outputlink>
                </apex:column>
                <apex:column value="{!e.City__c}"/>
                <apex:column value="{!e.Starting_Date__c}"/>
                <apex:column value="{!e.Price__c}"/>
            </apex:pageBlockTable>
        </apex:pageBlockSection>
    </apex:pageBlock>
</apex:page> 
